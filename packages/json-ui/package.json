{
  "name": "@actionbookdev/json-ui",
  "version": "0.1.0",
  "description": "Universal JSON-to-UI rendering components for AI Agent reports, based on json-render",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "json-ui": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./catalog": {
      "types": "./dist/catalog.d.ts",
      "import": "./dist/catalog.js"
    },
    "./cli": {
      "import": "./dist/cli.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "render": "node dist/cli.js render"
  },
  "dependencies": {
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "react": "^18.3.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "keywords": [
    "json-render",
    "ui",
    "report",
    "ai-agent",
    "actionbook",
    "react",
    "zod"
  ],
  "license": "MIT"
}
